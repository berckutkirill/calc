<form class="ajaxForm" method="post" action="/setDockPrice">
    {{> select data.series multiple='multiple'}}
    {{> select data.dock multiple='multiple'}}
    {{> select data.color multiple='multiple'}}
    {{> select data.furnish multiple='multiple'}}

    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="quantity">Количество</label>
            </div>
            <div class="col-6">
                <input id="quantity" name="quantity" class="form-control" value="1"/>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="wall_min_width">Минимальная ширина (мм)</label>
            </div>
            <div class="col-6">
                <input name="wall_min_width" id="wall_min_width" class="form-control" />
            </div>
        </div>
    </div>
      <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="wall_max_width">Максимальная ширина (мм)</label>
            </div>
            <div class="col-6">
                <input name="wall_max_width" id="wall_max_width"  class="form-control" />
            </div>
        </div>
    </div>
      <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="height">Высота (м)</label>
            </div>
            <div class="col-6">
                <input name="height" id="height" class="form-control" />
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="for_calc">Для расчетов</label>
            </div>
            <div class="col-6">
                <input type="checkbox" name="for_calc" id="for_calc" class="form-control" value="true"/>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="price">Цена</label>
            </div>
            <div class="col-6">
                <input name="price" id="price"  class="form-control" />
            </div>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

