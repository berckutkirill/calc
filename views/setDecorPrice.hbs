<form class="ajaxForm" method="post" action="/setDecorPrice">
    {{> select data.series multiple='multiple'}}
    {{> select data.decorative_element multiple='multiple'}}
    {{> select data.material multiple='multiple'}}
    {{> select data.color multiple='multiple'}}
    {{> select data.furnish multiple='multiple'}}
    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="pora">Пора</label>
            </div>
            <div class="col-6">
                <select id="pora" name="pora" class="form-control">
                    <option value="">---</option>
                    <option value="0">Закрытая</option>
                    <option value="1">Открытая</option>
                </select>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="width">Ширина</label>
            </div>
            <div class="col-6">
                <select name="width" id="width"  class="form-control">
                    <option value="">---</option>
                    <option value="70">70 мм</option>
                    <option value="80">80 мм</option>
                </select>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="patina">Патина</label>
            </div>
            <div class="col-6">
                <input type="checkbox" name="patina" id="patina" class="form-control" value="true"/>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <div class="col-6">
                <label for="price">Цена</label>
            </div>
            <div class="col-6">
                <input name="price" id="price"  class="form-control" />
            </div>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

